{
  "dependencies": {
    "error": "^0.1.0",
    "types": "^0.1.0"
  },
  "description": "Code generation from Crucible definitions",
  "exports": {
    "Generator": {
      "dependencies": [
        {
          "imports": [
            "Project",
            "Module"
          ],
          "module": "types"
        },
        {
          "imports": [
            "CrucibleError"
          ],
          "module": "error"
        }
      ],
      "methods": {
        "generate": {
          "calls": [
            "generator.generate_typescript",
            "generator.generate_rust"
          ],
          "effects": [
            "file.write"
          ],
          "inputs": [
            {
              "name": "project",
              "type": "types.Project"
            },
            {
              "name": "language",
              "type": "Language"
            },
            {
              "name": "output_dir",
              "type": "string"
            }
          ],
          "returns": {
            "inner": "void, error.CrucibleError",
            "type": "Result"
          }
        },
        "generate_rust": {
          "calls": [],
          "effects": [
            "file.write"
          ],
          "inputs": [
            {
              "name": "output_dir",
              "type": "string"
            }
          ],
          "returns": {
            "inner": "void, error.CrucibleError",
            "type": "Result"
          }
        },
        "generate_typescript": {
          "calls": [],
          "effects": [
            "file.write"
          ],
          "inputs": [
            {
              "name": "output_dir",
              "type": "string"
            }
          ],
          "returns": {
            "inner": "void, error.CrucibleError",
            "type": "Result"
          }
        }
      },
      "type": "class"
    },
    "Language": {
      "type": "enum",
      "values": [
        "TypeScript",
        "Rust",
        "Python",
        "Go",
        "Java"
      ]
    },
    "generate_typescript": {
      "description": "Auto-generated from code",
      "methods": {},
      "type": "function"
    },
    "new": {
      "description": "Auto-generated from code",
      "methods": {},
      "type": "function"
    }
  },
  "layer": "application",
  "module": "generator",
  "version": "0.1.0"
}