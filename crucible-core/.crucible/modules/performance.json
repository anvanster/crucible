          "returns": {
            "type": "void"
          },
          "throws": [],
          "calls": [],
          "effects": ["builds dependency graph"]
        },
        "detect_changes": {
          "inputs": [
            {
              "name": "project",
              "type": "Project",
              "optional": false,
