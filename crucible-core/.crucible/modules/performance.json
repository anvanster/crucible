{
  "module": "performance",
  "version": "0.1.0",
  "layer": "core",
  "description": "Performance optimization features including caching, incremental validation, and configuration management",
  "exports": {
    "ArchitectureCache": {
      "type": "class",
      "methods": {
        "new": {
          "inputs": [],
          "returns": {
            "type": "ArchitectureCache"
          },
          "throws": [],
          "calls": [],
          "effects": ["creates cache instance"]
        },
        "disabled": {
          "inputs": [],
          "returns": {
            "type": "ArchitectureCache"
          },
          "throws": [],
          "calls": [],
          "effects": ["creates disabled cache instance"]
        },
        "set_enabled": {
          "inputs": [
            {
              "name": "enabled",
              "type": "boolean",
              "optional": false,
              "description": "Enable or disable caching"
            }
          ],
          "returns": {
            "type": "void"
          },
          "throws": [],
          "calls": ["performance.ArchitectureCache.clear"],
          "effects": ["modifies cache state", "may clear cache"]
        },
        "get_module": {
          "inputs": [
            {
              "name": "path",
              "type": "Path",
              "optional": false,
              "description": "Path to module file"
            }
          ],
          "returns": {
            "type": "Option<Module>"
          },
          "throws": ["CrucibleError"],
          "calls": [],
          "effects": ["reads file metadata", "checks timestamps"]
        },
        "cache_module": {
          "inputs": [
            {
              "name": "path",
              "type": "PathBuf",
              "optional": false,
              "description": "Path to module file"
            },
            {
              "name": "module",
              "type": "Module",
              "optional": false,
              "description": "Module to cache"
            }
          ],
          "returns": {
            "type": "Result<void>"
          },
          "throws": ["CrucibleError"],
          "calls": [],
          "effects": ["reads file metadata", "stores in cache"]
        },
        "get_project": {
          "inputs": [
            {
              "name": "manifest_path",
              "type": "Path",
              "optional": false,
              "description": "Path to manifest.json"
            }
          ],
          "returns": {
            "type": "Option<Project>"
          },
          "throws": ["CrucibleError"],
          "calls": [],
          "effects": ["reads file metadata", "checks timestamps"]
        },
        "cache_project": {
          "inputs": [
            {
              "name": "manifest_path",
              "type": "Path",
              "optional": false,
              "description": "Path to manifest.json"
            },
            {
              "name": "project",
              "type": "Project",
              "optional": false,
              "description": "Project to cache"
            }
          ],
          "returns": {
            "type": "Result<void>"
          },
          "throws": ["CrucibleError"],
          "calls": [],
          "effects": ["reads file metadata", "stores in cache"]
        },
        "clear": {
          "inputs": [],
          "returns": {
            "type": "void"
          },
          "throws": [],
          "calls": [],
          "effects": ["clears all cached data"]
        },
        "stats": {
          "inputs": [],
          "returns": {
            "type": "CacheStats"
          },
          "throws": [],
          "calls": [],
          "effects": []
        }
      }
    },
    "CacheStats": {
      "type": "type",
      "properties": {
        "modules_cached": {
          "type": "usize",
          "required": true,
          "description": "Number of modules in cache"
        },
        "project_cached": {
          "type": "boolean",
          "required": true,
          "description": "Whether project is cached"
        },
        "enabled": {
          "type": "boolean",
          "required": true,
          "description": "Whether caching is enabled"
        }
      }
    },
    "ChangeTracker": {
      "type": "class",
      "methods": {
        "new": {
          "inputs": [],
          "returns": {
            "type": "ChangeTracker"
          },
          "throws": [],
          "calls": [],
          "effects": ["creates tracker instance"]
        },
        "build_dependency_graph": {
          "inputs": [
            {
              "name": "project",
              "type": "Project",
              "optional": false,
              "description": "Project to analyze"
            }
