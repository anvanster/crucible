        },
        "default_validation_level": {
          "type": "ValidationLevel",
          "required": true,
          "description": "Default validation severity"
        },
        "template_paths": {
          "type": "Vec<String>",
          "required": true,
          "description": "Paths to template directories"
        },
        "performance": {
          "type": "PerformanceConfig",
          "required": true,
          "description": "Performance settings"
        }
      }
    },
    "HashMap<string, boolean>": {
      "type": "type",
      "properties": {}
    },
    "Vec<string>": {
      "type": "type",
      "properties": {}
    },
    "PerformanceConfig": {
      "type": "type",
      "properties": {
        "enable_caching": {
          "type": "boolean",
          "required": true,
          "description": "Enable caching"
        },
        "cache_ttl_seconds": {
          "type": "u64",
          "required": true,
          "description": "Cache TTL in seconds"
        },
        "lazy_loading": {
          "type": "boolean",
          "required": true,
          "description": "Enable lazy loading"
        },
        "incremental_validation": {
          "type": "boolean",
          "required": true,
          "description": "Enable incremental validation"
        },
        "max_parallel_modules": {
          "type": "usize",
          "required": true,
          "description": "Max parallel module processing"
        }
      }
    }
  },
  "dependencies": {}
}