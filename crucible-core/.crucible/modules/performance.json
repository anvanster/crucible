          "throws": [],
          "calls": [],
          "effects": ["analyzes dependencies"]
        },
        "update_timestamps": {
          "inputs": [
            {
              "name": "validated_modules",
              "type": "Vec<string>",
              "optional": false,
              "description": "Successfully validated modules"
            }
          ],
          "returns": {
            "type": "void"
          },
          "throws": [],
          "calls": [],
          "effects": ["updates internal timestamps"]
        }
      }
    },
    "GlobalConfig": {
      "type": "type",
      "properties": {
        "enabled": {
          "type": "boolean",
          "required": true,
          "description": "Whether integration is enabled"
        },
        "auto_discover": {
          "type": "boolean",
          "required": true,
          "description": "Auto-discover projects"
        },
        "default_mode": {
          "type": "IntegrationMode",
          "required": true,
